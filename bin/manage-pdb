#!/Volumes/Data/slowevents/api/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pdb==0.1','console_scripts','manage-pdb'
__requires__ = 'pdb==0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pdb==0.1', 'console_scripts', 'manage-pdb')()
    )
